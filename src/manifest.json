{
	"name":"SearchBox Bookmarks",
	"version":"@version@",
	"manifest_version":2,
	"description":"Bookmark search-boxes and use the search-box in the toolbar popup.",
	"icons":{
		"32":"32.png",
		"48":"48.png",
		"128":"128.png"
	},
	"background":{
		"scripts":["b.js"],
		"persistent":false
	},
	"browser_action":{
		"default_popup":"p.html",
		"default_icon":"a.png"
	},
	"permissions":[
		"tabs",
		"http://*/*",
		"https://*/*",
		"contextMenus"
	]
}
